class Factorial
{
	void main()
	{
		int x;

		x = factorial(6);

		print "Factorial de ""6"" = ", x;
	}

	// Funcion que calcula el factorial
	int factorial(int n)
	{
		print "if";
		if (n == 0) {
			return (1);
			print "a";
		} else {
			return n * factorial(n-1);
			print "b ", n;
		}
		print 40;
	}
}
